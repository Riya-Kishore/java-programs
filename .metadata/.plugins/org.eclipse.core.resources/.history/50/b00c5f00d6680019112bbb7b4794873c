package factorial;
import java.util.Scanner;

public class factorial {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int num, temp, fact;
		System.out.println("Enter Number :- ");
		num = s.nextInt();
		temp = num;
		while(num >= 1)
		{
			fact = fact *num;
			num--;
		}
		System.out.println("Factorial of a number : " + temp + " is = " + fact);
	}

}
