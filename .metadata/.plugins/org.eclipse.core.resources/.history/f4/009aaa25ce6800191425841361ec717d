package even_odd;
import java.util.Scanner;

public class even_odd {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int num;
		num = s.nextInt();
		if(num%2==0)
		{
			System.out.println(num + " " + " is an even number");
		}
		else
		{
			System.out.println(num + " " + " is an odd number");
		}
	}

}
